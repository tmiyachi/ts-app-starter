{
  "name": "ts-app-starter",
  "version": "1.0.0",
  "description": "Starter kit for TypeScript web application",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^16.11.51",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.22.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.ts",
    "build:dev": "webpack --config webpack.dev.ts --watch",
    "start": "webpack serve --open --config webpack.dev.ts",
    "lint:eslint": "eslint src/**/*.{ts,js}",
    "lint:stylelint": "stylelint src/**/*.{css,scss}",
    "lint:prettier": "prettier src/**/*.{ts,js,css,scss} --check",
    "lint": "run-p lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-s fix:*"
  },
  "private": true,
  "keywords": [],
  "author": "tmiyachi",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.24.1"
  }
}
